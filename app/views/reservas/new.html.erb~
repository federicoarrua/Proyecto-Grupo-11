  </p>
 
  <p>
    <%= f.label :Salida %><br>
    <%= f.date_select :salida, :order => [:day, :month, :year], :start_year=>2015 %>
  </p>
 	
	params[:id]=>couch_id 

  <p>
    <%= f.submit %>
  </p>
<% end %>


